#import "SearchTypeDataController.h"

@implementation SearchTypeDataController

@synthesize searchTypeArray = _searchTypeArray;

- (id)init {
    if (self = [super init]) {
        [self initialiseSearchTypeArray];
    }
    return self;
}

- (void)initialiseSearchTypeArray {
    self.searchTypeArray = [[NSMutableArray alloc] init];
    
    [self.searchTypeArray addObject:@"Appointment Date"];
    [self.searchTypeArray addObject:@"Clinician"];
    [self.searchTypeArray addObject:@"Clinic / Surgery"];
}

@end
